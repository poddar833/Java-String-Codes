import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        
        Scanner sc=new Scanner(System.in);
        String A=sc.next();
        String B=sc.next();
        int len_res = A.length() + B.length();
        System.out.println(len_res);
        if(A.compareTo(B) > 0)
            System.out.println("Yes");
        else 
            System.out.println("No");
      
        System.out.println(A.substring(0,1).toUpperCase() + A.substring(1) + " " + B.substring(0,1).toUpperCase() + B.substring(1));
        

        
    }
}

// Here in this code , we have capitalised the first character of each string
// subString(0,1).toUpperCase() +  subString(1);
// subString(0,n+1)=> It will select character from 0 to n as it will not include n+1
// subString(0)=> It will select character from 0 to string.length();



